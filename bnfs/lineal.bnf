<S>                 ::= <expr>
<expr>              ::= <expr> + <expr> | <exp> | <exp_senoidal>
<exp>               ::= <real> * math.exp(<x>) | <x> * math.exp(<x>)
<exp_senoidal>      ::= math.exp(<x>) * (<real>*math.cos(<x>) + <real>*math.sin(<x>))
                        x * math.exp(<x>) * (<real>*math.cos(<x>) + <real>*math.sin(<x>))
<x>                 ::= <real> * x | x
<real>              ::= <int> | - <int>
<int>         ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9 | 0 | <int><int>
